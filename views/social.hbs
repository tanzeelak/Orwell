  <iframe name="hiddenFrame" width="0" height="0" border="0" class="hide"></iframe>
  <div id="whos-watching">
    <div id="prompt-wrapper">
      <h1 class="prompt">Who's watching?</h1>
      <form action="/insert" method="post" target="hiddenFrame" id="who-form" class="">
        <input id="prompt-input" type="text" name="username" value="">
        <input id="prompt-submit" type="submit" value="Enter" onclick="showNamesList()">
      </form>
    </div>
    <div id="names-wrapper" class="hide">
      <h1 class="prompt">Who's watching?</h1>
      <p id="input-name"></p>
      <div id="names-list">
        <div class="name-column">
          {{#each items}}
          <p>
            {{this.username}}
          </p>
          {{/each}}
        </div>
        <div class="name-column">
